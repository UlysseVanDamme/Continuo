# Backend API Environment Variables
# Copy this file to .env and fill in your actual values

# Database connection string (PostgreSQL)
# Example: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://user:password@localhost:5432/continuo

# API key for ingestion endpoint (generate a secure random string)
INGEST_API_KEY=your-secure-api-key-here

# Frontend website URL for CORS configuration
WEBSITE_LINK=http://localhost:3000

# S3 bucket name for MIDI file storage
S3_BUCKET_NAME=your-midi-bucket-name
