# Worker Environment Variables
# Copy this file to .env and fill in your actual values

# Database connection string (PostgreSQL)
# Example: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://user:password@localhost:5432/continuo

# SQS queue URL for processing jobs
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/your-queue-name

# AWS credentials are typically provided via IAM roles for ECS/Fargate
# If running locally, set AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY
# AWS_ACCESS_KEY_ID=AKIAXXXXXXXXXXXXXXXX
# AWS_SECRET_ACCESS_KEY=your-secret-key-here
# AWS_REGION=us-east-1
