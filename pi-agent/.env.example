# Pi Agent Environment Variables
# Copy this file to .env and fill in your actual values

# MIDI port name (use 'python -m mido.ports' to list available ports)
PORT_NAME=Your MIDI Device Name

# S3 bucket name for uploading MIDI files
BUCKET_NAME=your-midi-bucket-name

# AWS credentials (or use IAM roles for EC2)
AWS_ACCESS_KEY=AKIAXXXXXXXXXXXXXXXX
AWS_SECRET_KEY=your-secret-key-here
AWS_REGION=us-east-1
